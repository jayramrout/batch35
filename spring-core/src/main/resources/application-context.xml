<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

        <bean id="buildCar" class="jrout.tutorial.batch35.springcore.BuildCar">
                <property name="engine" ref="4se"/>
        </bean>

        <bean id="2se" class="jrout.tutorial.batch35.springcore.TwoStrokeEngine">
                <property name="engineType" value="2 S E" />
        </bean>

        <bean id="4se" class="jrout.tutorial.batch35.springcore.FourStrokeEngine">
                <property name="engineType" value="4 S E" />
        </bean>

</beans>